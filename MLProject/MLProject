conda_env: conda.yaml

entry_points:
  main:
    parameters:
      data_dir:
        type: path
        default: data
      n_estimators:
        type: int
        default: 100
      max_depth:
        type: int
        default: 10
    command: >
      python modelling.py
      --data_dir {data_dir}
      --n_estimators {n_estimators}
      --max_depth {max_depth}
